{
  "compilerOptions": {
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "sourceMap": true,
    "removeComments": false,
    "lib": [ "dom" ],
    "target": "es2018",
    "outDir": "wwwroot/js"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": true
}