{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "sourceMap": true,
    "removeComments": false,
    "lib": [ "dom" ],
    "target": "es5",
    "allowJs": true,
    "outDir": "wwwroot/js"
    //"outFile": "wwwroot/js/bundle.js"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": true
}