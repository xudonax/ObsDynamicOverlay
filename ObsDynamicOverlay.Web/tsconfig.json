{
  "compilerOptions": {
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "sourceMap": true,
    "removeComments": true,
    "jsx": "react",
    "lib": [ "dom" ],
    "target": "es6",
    "allowJs": true,
    "outDir": "dist"
  },
  "exclude": [
    "dist",
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": false
}